{% if profile.email %}
  <a href="mailto:{{ profile.email | encode_email }}" title="email"><i class="fa-solid fa-envelope"></i></a>
{% endif %}
{% if profile.telegram_username %}
  <a href="https://telegram.me/{{ profile.telegram_username }}" title="telegram"><i class="fa-brands fa-telegram"></i></a>
{% endif %}
{% if profile.whatsapp_number %}
  <a href="https://wa.me/{{ profile.whatsapp_number }}" title="whatsapp"><i class="fa-brands fa-whatsapp"></i></a>
{% endif %}
{% if profile.orcid_id %}
  <a href="https://orcid.org/{{ profile.orcid_id }}" title="ORCID"><i class="ai ai-orcid"></i></a>
{% endif %}
{% if profile.scholar_userid %}
  <a href="https://scholar.google.com/citations?user={{ profile.scholar_userid }}" title="Google Scholar"><i class="ai ai-google-scholar"></i></a>
{% endif %}
{% if profile.semanticscholar_id %}
  <a href="https://www.semanticscholar.org/author/{{ profile.semanticscholar_id }}" title="Semantic Scholar"><i class="ai ai-semantic-scholar"></i></a>
{% endif %}
{% if profile.publons_id %}
  <a href="https://publons.com/a/{{ profile.publons_id }}/" title="Publons"><i class="ai ai-publons"></i></a>
{% endif %}
{% if profile.lattes_id %}
  <a href="http://lattes.cnpq.br/{{ profile.lattes_id }}" target="_blank" title="Lattes"><i class="ai ai-lattes"></i></a>
{% endif %}
{% if profile.osf_id %}
  <a href="https://osf.io/{{ profile.osf_id }}/" title="Open Science Framework"><i class="ai ai-osf"></i></a>
{% endif %}
{% if profile.research_gate_profile %}
  <a href="https://www.researchgate.net/profile/{{profile.research_gate_profile}}/" title="ResearchGate"><i class="ai ai-researchgate"></i></a>
{% endif %}
{% if profile.ieee_id %}
  <a href="https://ieeexplore.ieee.org/author/{{profile.ieee_id}}/" title="IEEE Xplore"><i class="ai ai-ieee"></i></a>
{% endif %}
{% if profile.acm_id %}
  <a href="https://dl.acm.org/profile/{{profile.acm_id}}/" title="ACM DL"><i class="ai ai-acm"></i></a>
{% endif %}
{% if profile.scopus_id %}
  <a href="https://www.scopus.com/authid/detail.uri?authorId={{profile.scopus_id}}" title="Scopus"><i class="ai ai-scopus"></i></a>
{% endif %}
{% if profile.github_username %}
  <a href="https://github.com/{{ profile.github_username }}" title="GitHub"><i class="fa-brands fa-github"></i></a>
{% endif %}
{% if profile.linkedin_username %}
  <a href="https://www.linkedin.com/in/{{ profile.linkedin_username }}" title="LinkedIn"><i class="fa-brands fa-linkedin"></i></a>
{% endif %}
{% if profile.x_username %}
  <a href="https://twitter.com/{{ profile.x_username }}" title="X"><i class="fa-brands fa-x-twitter"></i></a>
{% endif %}
{% if profile.mastodon_username %}
  <a rel="me" href="https://{{ profile.mastodon_username  }}" title="Mastodon"><i class="fa-brands fa-mastodon"></i></a>
{% endif %}
{% if profile.medium_username %}
  <a href="https://medium.com/@{{ profile.medium_username }}" title="Medium"><i class="fa-brands fa-medium"></i></a>
{% endif %}
{% if profile.quora_username %}
  <a href="https://www.quora.com/profile/{{ profile.quora_username }}" title="Quora"><i class="fa-brands fa-quora"></i></a>
{% endif %}
{% if profile.flickr_id %}
  <a href="https://www.flickr.com/{{ profile.flickr_id }}" title="Flickr"><i class="fa-brands fa-flickr"></i></a>
{% endif %}
{% if profile.blogger_url %}
  <a href="{{ profile.blogger_url }}" title="Blogger"><i class="fa-brands fa-blogger-b"></i></a>
{% endif %}
{% if profile.work_url %}
  <a href="{{ profile.work_url }}" title="Work"><i class="fa-solid fa-briefcase"></i></a>
{% endif %}
{% if profile.wikidata_id %}
  <a href="https://www.wikidata.org/wiki/{{ profile.wikidata_id }}" title="Wikidata"><i class="fa-solid fa-barcode"></i></a>
{% endif %}
{% if profile.wikipedia_id %}
  <a href="https://wikipedia.org/wiki/User:{{ profile.wikipedia_id }}" title="Wikipedia"><i class="fa-brands fa-wikipedia-w"></i></a>
{% endif %}
{% if profile.strava_userid %}
  <a href="https://www.strava.com/athletes/{{ profile.strava_userid }}" title="Strava"><i class="fa-brands fa-strava"></i></a>
{% endif %}
{% if profile.keybase_username %}
  <a href="https://keybase.io/{{ profile.keybase_username }}" title="Keybase"><i class="fa-brands fa-keybase"></i></a>
{% endif %}
{% if profile.gitlab_username %}
  <a href="https://gitlab.com/{{ profile.gitlab_username }}" title="GitLab"><i class="fa-brands fa-gitlab"></i></a>
{% endif %}
{% if profile.dblp_url %}
  <a href="{{ profile.dblp_url }}" title="DBLP"><i class="ai ai-dblp"></i></a>
{% endif %}
{% if profile.stackoverflow_id %}
  <a href="https://stackoverflow.com/users/{{ profile.stackoverflow_id }}" title="Stackoverflow"><i class="fa-brands fa-stack-overflow"></i></a>
{% endif %}
{% if profile.kaggle_id %}
  <a href="https://www.kaggle.com/{{ profile.kaggle_id }}" title="Kaggle"><i class="fa-brands fa-kaggle"></i></a>
{% endif %}
{% if profile.lastfm_id %}
  <a href="https://www.last.fm/user/{{ profile.lastfm_id }}" title="Last FM"><i class="fa-brands fa-lastfm"></i></a>
{% endif %}
{% if profile.spotify_id %}
  <a href="https://open.spotify.com/user/{{ profile.spotify_id }}" title="Spotify"><i class="fa-brands fa-spotify"></i></a>
{% endif %}
{% if profile.pinterest_id %}
  <a href="https://www.pinterest.com/{{ profile.pinterest_id }}" title="Pinterest"><i class="fa-brands fa-pinterest"></i></a>
{% endif %}
{% if profile.unsplash_id %}
  <a href="https://unsplash.com/@{{ profile.unsplash_id }}" title="Unsplash"><i class="fa-brands fa-unsplash"></i></a>
{% endif %}
{% if profile.instagram_id %}
  <a href="https://instagram.com/{{ profile.instagram_id }}" title="Instagram"><i class="fa-brands fa-instagram"></i></a>
{% endif %}
{% if profile.facebook_id %}
  <a href="https://facebook.com/{{ profile.facebook_id }}" title="Facebook"><i class="fa-brands fa-facebook"></i></a>
{% endif %}
{% if profile.bluesky_url %}
  <a href="https://bsky.app/profile/{{ profile.bluesky_url }}" title="Bluesky"><i class="fa-brands fa-bluesky"></i></a>
{% endif %}
{% if profile.youtube_id %}
  <a href="https://youtube.com/@{{ profile.youtube_id }}" title="YouTube"><i class="fa-brands fa-youtube"></i></a>
{% endif %}
{% if profile.discord_id %}
  <a href="https://discord.com/users/{{ profile.discord_id }}" title="Discord"><i class="fa-brands fa-discord"></i></a>
{% endif %}
{% if profile.zotero_username %}
  <a href="https://www.zotero.org/{{ profile.zotero_username }}" title="Zotero"><i class="ai ai-zotero"></i></a>
{% endif %}
{% if profile.rss_icon %}
  <a href="{{ profile.baseurl }}/feed.xml" title="RSS Feed"><i class="fa-solid fa-square-rss"></i></a>
{% endif %}
{% if profile.wechat_qr %}
  <a id="WeChatBtn" title="WeChat"><i class="fa-brands fa-weixin"></i></a>
  <div id="WeChatMod" class="wechat-modal">
    <img src="{{ profile.wechat_qr | prepend: 'assets/img/' | relative_url }}" alt="WeChat QR" id="WeChatQR">
  </div>
  {% include scripts/wechatModal.liquid %}
{% endif %}
